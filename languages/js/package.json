{
  "name": "oso",
  "version": "0.12.2",
  "description": "oso authorization library.",
  "bin": "bin/repl.js",
  "main": "dist/src/index.js",
  "browser": {
    "fs": false,
    "readline": false,
    "repl": false,
    "./dist/src/polar_wasm_api.js": "./dist/bundler/polar_wasm_api.js",
    "./dist/src/polar_wasm_api_bg.js": "./dist/bundler/polar_wasm_api_bg.js"
  },
  "repository": "https://github.com/osohq/oso",
  "collaborators": [
    "Oso Security <support@osohq.com>"
  ],
  "license": "Apache-2.0",
  "private": false,
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "fmt": "prettier --write 'src/**/*.[jt]s' 'test/**/*.[jt]s'",
    "fmtcheck": "prettier --check 'src/**/*.[jt]s' 'test/**/*.[jt]s'",
    "test": "jest",
    "docs-build": "typedoc --options typedoc.js",
    "docs-watch": "chokidar 'src/**/*.ts' -c 'yarn docs-build'",
    "docs-serve": "live-server docs",
    "build": "yarn tsc --project tsconfig.build.json"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.14",
    "chokidar-cli": "^2.1.0",
    "gts": "^2.0.2",
    "jest": "^26.3.0",
    "live-server": "^1.2.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "temp-write": "^4.0.0",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.4",
    "typescript": "^3.9.5"
  }
}
