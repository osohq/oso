{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": [
      "DOM",
      "ES2018"
    ],
    "noEmit": true,
    "outDir": "dist",
    "rootDir": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src/**/*",
    "test/**/*"
  ]
}
