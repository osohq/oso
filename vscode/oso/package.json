{
  "name": "oso",
  "publisher": "Oso Security, Inc.",
  "displayName": "oso",
  "description": "Syntax highlighting for .polar files as used by oso.",
  "icon": "oso_logo.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "oso",
        "aliases": [
          "Oso",
          "oso"
        ],
        "extensions": [
          ".polar"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "oso",
        "scopeName": "source.oso",
        "path": "./syntaxes/oso.tmLanguage.json"
      }
    ]
  }
}
